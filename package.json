    "build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets",
    "build:assets": "node justinTestSite/scripts/build-assets.js",
    "build:pug": "node justinTestSite/scripts/build-pug.js",
    "build:scripts": "node justinTestSite/scripts/build-scripts.js",
    "build:scss": "node justinTestSite/scripts/build-scss.js",
    "clean": "node justinTestSite/scripts/clean.js",
    "start": "npm run build && node justinTestSite/scripts/start.js",
    "start:debug": "npm run build && node justinTestSite/scripts/start-debug.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikaylabryant1/testProjects.git"
